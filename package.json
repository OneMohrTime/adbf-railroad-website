{
  "name": "adbf-tailwind",
  "version": "1.0.0",
  "description": "Timber WordPress theme with Tailwind CSS.",
  "author": {
    "name": "Mighty in the Midwest",
    "email": "developers@mightyinthemidwest.com",
    "url": "https://mightyinthemidwest.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jeffreyvr/tailpress"
  },
  "text_domain": "adbf",
  "license": "MIT",
  "browserslist": [
    "last 5 versions",
    "> 1%",
    "IE 10-11"
  ],
  "scripts": {
    "production:css-app": "cross-env NODE_ENV=production tailwindcss -i ./web/app/themes/adbf-railroad/src/styles/app-tailwind.css -o ./web/app/themes/adbf-railroad/css/app.css --postcss --minify",
    "production:css-editor": "cross-env NODE_ENV=production tailwindcss -i ./web/app/themes/adbf-railroad/src/styles/editor-style.css -o ./web/app/themes/adbf-railroad/css/editor-style.css --postcss --minify",
    "production:js": "cross-env NODE_ENV=production ./node_modules/.bin/esbuild ./web/app/themes/adbf-railroad/src/scripts/app.js --bundle --outfile=./web/app/themes/adbf-railroad/js/app.js --minify",
    "dev:css-app": "cross-env NODE_ENV=development tailwindcss -i ./web/app/themes/adbf-railroad/src/styles/app-tailwind.css -o ./web/app/themes/adbf-railroad/css/app.css --postcss",
    "dev:css-editor": "cross-env NODE_ENV=development tailwindcss -i ./web/app/themes/adbf-railroad/src/styles/editor-style.css -o ./web/app/themes/adbf-railroad/css/editor-style.css --postcss",
    "dev:js": "cross-env NODE_ENV=development ./node_modules/.bin/esbuild ./web/app/themes/adbf-railroad/src/scripts/app.js --bundle --outfile=./web/app/themes/adbf-railroad/js/app.js",
    "watch:css-app": "cross-env NODE_ENV=development tailwindcss -i ./web/app/themes/adbf-railroad/src/styles/app-tailwind.css -o ./web/app/themes/adbf-railroad/css/app.css --postcss --watch",
    "watch:css-editor": "cross-env NODE_ENV=development tailwindcss -i ./web/app/themes/adbf-railroad/src/styles/editor-style.css -o ./web/app/themes/adbf-railroad/css/editor-style.css --postcss --watch",
    "watch:js": "cross-env NODE_ENV=development ./node_modules/.bin/esbuild ./web/app/themes/adbf-railroad/src/scripts/app.js --bundle --outfile=./web/app/themes/adbf-railroad/js/app.js --watch",
    "production": "cross-env NODE_ENV=production concurrently \"npm run production:css-app\" \"npm run production:css-editor\" \"npm run production:js\"",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run dev:css-app\" \"npm run dev:css-editor\" \"npm run dev:js\"",
    "watch": "cross-env NODE_ENV=development concurrently \"npm run watch:css-app\" \"npm run watch:css-editor\" \"npm run watch:js\"",
    "browser-sync": "cross-env NODE_ENV=development browser-sync start --proxy \"tailpress.test\" --host=\"tailpress.test\" --no-inject-changes --files=\"./\"",
    "watch-sync": "cross-env NODE_ENV=development concurrently \"npm run browser-sync\" \"npm run watch\""
  },
  "devDependencies": {
    "@jeffreyvr/tailwindcss-tailpress": "^2.0.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "autoprefixer": "^10.4.0",
    "browser-sync": "^2.26.14",
    "concurrently": "^6.2.1",
    "cross-env": "^6.0.3",
    "esbuild": "^0.12.24",
    "postcss": "^8.4.4",
    "postcss-import": "^14.0.0",
    "postcss-nested-ancestors": "^2.0.0",
    "resolve-url-loader": "^3.1.2",
    "tailwindcss": "^3.1.0"
  },
  "dependencies": {
    "locomotive-scroll": "^4.1.4",
    "modujs": "^1.4.2",
    "modularload": "^1.2.8",
    "modularscroll": "^1.0.7"
  }
}
